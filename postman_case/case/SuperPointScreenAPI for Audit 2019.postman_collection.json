				{
					"name": "Daily Ads API",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "91317c7b-d52b-4804-b8ac-9df2e1e4dad2",
								"exec": [
									"// 広告種類定義",
									"var Point_ad = 1;       // ポイント付き広告",
									"var Recommend = 2;      // おすすめ広告",
									"var Hot = 3;            // 人気広告",
									"var Tamaru = 4;         // P貯まる広告",
									"var Coupon = 5;         // クーポン広告",
									"var PR = 6;             // PR広告",
									"var Article = 7;        // 読みもの",
									"",
									"// Response Body取得",
									"var data = JSON.parse(responseBody);",
									"var arrayList  = data.results;",
									"",
									"// ボタンタイプ変数定義",
									"var buttonType;",
									"// ボタンタイプリスト変数定義",
									"var buttonTypeList = [];",
									"",
									"for (var i in arrayList){",
									"    // ボタンタイプ取得",
									"    buttonType = arrayList[i].detail.buttonType;",
									"    // ボタンタイプリスト取得",
									"    buttonTypeList[i] = buttonType;",
									"}",
									"",
									"pm.test(\"ポイント付き広告存在チェック\", function() {",
									"    // ポイント付き広告が存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(Point_ad);",
									"});",
									"",
									"pm.test(\"おすすめ広告存在チェック\", function() {",
									"    // おすすめ広告が存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(Recommend);",
									"});",
									"",
									"pm.test(\"人気広告存在チェック\", function() {",
									"    // 人気広告が存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(Hot);",
									"});",
									"",
									"pm.test(\"P貯まる広告存在チェック\", function() {",
									"    // P貯まる広告が存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(Tamaru);",
									"});",
									"",
									"pm.test(\"クーポン広告存在チェック\", function() {",
									"    // クーポン広告が存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(Coupon);",
									"});",
									"",
									"pm.test(\"PR広告存在チェック\", function() {",
									"    // PR広告が存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(PR);",
									"});",
									"",
									"pm.test(\"読みもの存在チェック\", function() {",
									"    // 読みものが存在するかどうかを判断する",
									"    pm.expect(buttonTypeList).to.include(Article);",
									"});"
								],
								"type": "text/javascript"
							}
						}
